<launch>
    <!-- 与 demo03 一起启动 -->

    <!-- 添加点云坐标系到 kinect 连杆坐标系的变换 -->
    <node name="static_transform_publisher" pkg="tf2_ros" type="static_transform_publisher" args="0 0 0 -1.57 0 -1.57 /support /support_depth" />

    <!-- 2、启动 rviz -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find two_urdf02_gazebo)/config/car_with_sensors.rviz" />
    
    <!-- 3、添加关节状态发布节点 -->
    <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher" />

    <!-- 4、添加机器人状态发布节点 -->
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />


</launch>